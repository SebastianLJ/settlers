<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LobbyController">
   <children>
      <VBox alignment="CENTER" fillWidth="false" layoutX="84.0" layoutY="100.0" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="84.0" AnchorPane.rightAnchor="83.0" AnchorPane.topAnchor="100.0">
         <children>
            <Label alignment="CENTER" text="Settlers of Catan">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Button fx:id="CreateGameButton" mnemonicParsing="false" onMouseClicked="#createGame" onMouseEntered="#onMouseEntered" onMouseExited="#onMouseExited" prefHeight="50.0" prefWidth="100.0" text="Create Game">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="JoinGameButton" mnemonicParsing="false" onMouseClicked="#joinGame" onMouseEntered="#onMouseEntered" onMouseExited="#onMouseExited" prefHeight="50.0" prefWidth="100.0" text="Join Game" />
         </children>
      </VBox>
   </children>
</AnchorPane>
